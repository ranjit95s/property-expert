<!DOCTYPE html>
<html>

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <link rel="stylesheet" href="/css/style.css">

        <link rel="stylesheet" type="text/css" href="/css/contact.css">
        <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
        <link href="https://fonts.googleapis.com/css?family=Poppins:600&display=swap" rel="stylesheet">
        <script src="https://kit.fontawesome.com/a81368914c.js"></script>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css">
        <title> PropertyExpert.Com </title>
        <style>
            .login-content .input-div {
                margin: 0 0 !important;
            }
            .input-div > div > input {
                border: 2px solid orangered;
                border-radius: 15px;
            }
            .i {
                color: orangered;
            }
        </style>
      </head>

      <body style="user-select: none !important;">
        <div style="height: 65px;">
          </div>
        <div class="container">
            <div class="login-content">
                <form action="/deptlogin/" method="POST" autocomplete="off">
                    <img src="/images/avatar.svg">

                    <% if(typeof err !='undefined' ){ %>

                        <div class="alert alert-<%= typeof errType != 'undefined' ? errType : " warning" %>
                            alert-dismissible fade show" role="alert">
                            <strong>
                                <%= typeof errHead !='undefined' ? errHead : "Error!" %>
                            </strong><br>
                           <h4><%= err %></h4> 
                        </div>
                        <% }%>
                            <% if(typeof err=='undefined' ){ %>
    
                                <h4> Only For Department Stuff !!!</h4>
    
                                <% }%>
                                    <div class="input-div one">
                                        <div class="i">
                                            <i class="fas fa-user"></i>
                                        </div>
                                        <div class="div">
                                            <input class="input" type="text" name="ad0" id="name" style="text-transform: none;">
                                        </div>
                                    </div>
                                    <div class="input-div pass">
                                        <div class="i">
                                            <i class="fas fa-lock"></i>
                                        </div>
                                        <div class="div">
                                            <input class="input" type="password" name="ad1" id="name">
                                        </div>
                                    </div>
                                    <input type="submit" class="btn" value="Send">
                </form>
            </div>
        </div>
        <script type="text/javascript" src="js/mainlog.js"></script>
    </body>
    <% if( typeof success !="undefined" ){ %>
        <script>
            var success = "<%= success %>";
            if (success == "false") {
                swal("LogIn fail !!!", "username or password is wrong, please try again !", "info");
            }
            else {
                swal("PASSWORD MATCH", "success");
            }
        </script>
        <% } %>
<!-- <script type="text/javascript" src="js/mainlog.js"></script> -->
<script src="../jq/jquery.js" type="text/javascript"></script>
<script src="../js/bootstrap.js" type="text/javascript"></script>
<!--jq cnd link-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<!--custom js link-->
<script src="js/main.js"></script>

</html>