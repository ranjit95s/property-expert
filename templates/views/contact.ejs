<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<% if( typeof success !="undefined" ){ %>
	<script>
		var success = "<%= success %>";
		if (success == "false") {
			swal("Please Enter Valid Mobile Number", "You entered mobile number is not in proper format. Please enter again.", "info");
		}
		else {
			swal("Message Sent", "Your message successfully sent to our servers. Thank You", "success")
				.then((value) => {
					if (value == true) {
						window.location.href = "/"
					} else {
						window.location.href = "/"
					}
				});
		}
	</script>
	<% } %>

