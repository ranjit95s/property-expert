    <section class="property" id="property">
      <h1 class="heading"><span>property</span></h1>
      <div class="slogan">

        <h2>Shave Time.. Shave Money..</h2>
        <h3>The Quicker Picker Upper!!! </h3>
      </div>
      <ul class="control">
        <li class="button button-active" data-filter="all">all</li>
        <li class="button" data-filter="luxery">luxery house</li>
        <li class="button" data-filter="4bhk">4 BHK</li>
        <li class="button" data-filter="3bhk">3 BHK</li>
        <li class="button" data-filter="2bhk">2 BHK</li>
        <li class="button" data-filter="1bhk">1 BHK</li>
        <li class="button" data-filter="1rk">1 RK</li>
      </ul>
      <div class="img-con">
        <% getHome.forEach(function (home){ %>
          <div class="image img-hover-zoom <%= home.home_Type%>">
            <img src="/images/TempUploads/<%=home.first_image%>" alt="images doesn't save long-term. Heroku's filesystem is ephemeral: any changes you make will be lost the next time your dyno restarts, which happens frequently (at least once per day).">
            <p class="building-name"><%= home.owner_name%>'s home</p>
            <p><%=home.sellOrRent%> Amount: <i class="fas fa-rupee-sign"></i> <%=home.offered_at%> </p>
            <div class="conatiner">
              <p class="glass"><%= home.home_Type%></p>
              <p class="glass"> <%=home.sq_ft%> sq.ft </p>
              <p class="glass"> location : <%=home.area_name%>,<%=home.city_name%></p>
            </div>
            <div class="stick_to_bo">
              <a href="/home?h_id=<%= home._id %>">View Full Details </a>
            </div>
          </div>
          <% }) %>
      </div>
      <div id="no_data" class="no_data hidden">
        <i class="far fa-sad-tear"></i>
        <p>Sorry! No Data Found.</p>
    </div>














